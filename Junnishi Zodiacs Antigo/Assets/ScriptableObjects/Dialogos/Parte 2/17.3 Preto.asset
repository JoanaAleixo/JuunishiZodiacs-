  m_Script: {fileID: 11500000, guid: 5cf4dd03915454544b5d06e9dbbfe34d, type: 3}
  m_Name: 17.3 Preto
  m_EditorClassIdentifier: 
  dialogoID: 1702
  dialogue:
  - speakerNome: Akira
    fala: "In front of me I saw the creature change its form, becoming humanoid\u2026.
      I recognize this figure... It's the person I came across on the day of the
      attack on Central Avenue..."
    personagemImagem: {fileID: 21300000, guid: 5d6195196bb237941b9a679afe7b424e, type: 3}
    caixaDialogo: {fileID: 21300000, guid: 53d00c5f52142284cb42bac33715bfae, type: 3}
  - speakerNome: Akira
    fala: "Our bodies glow, I can feel an intense heat in my arms \u2013 my bandages
      fall off without anyone touching them. It's strange to look at my scars on
      my hands without repudiation... without emotion against them..."
    personagemImagem: {fileID: 21300000, guid: 5d6195196bb237941b9a679afe7b424e, type: 3}
    caixaDialogo: {fileID: 21300000, guid: 53d00c5f52142284cb42bac33715bfae, type: 3}
  - speakerNome: Akira
    fala: "Nezuko reaches out to me \u2013 he has a sweet smile and deep coal-black
      eyes. As soon as I touch his hand, two mouse silhouettes appear in my arms.
      The glow caused by them is intense\u2026 This is not a dream\u2026 Is this
      the future that awaits me\u2026?"
    personagemImagem: {fileID: 21300000, guid: 5d6195196bb237941b9a679afe7b424e, type: 3}
    caixaDialogo: {fileID: 21300000, guid: 53d00c5f52142284cb42bac33715bfae, type: 3}
  - speakerNome: Nezuko
    fala: '"Now you and I are one, Akira, welcome to your second life as a Zodiac."'
    personagemImagem: {fileID: 21300000, guid: 17d69d746b9828646ae998aa667be909, type: 3}
    caixaDialogo: {fileID: 21300000, guid: 152fa238407d6c44bae8795c4f6b8521, type: 3}
